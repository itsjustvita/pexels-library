{
    "name": "webeweb/pexels-library",
    "type": "library",
    "description": "Integrate Pexels API with your projects",
    "keywords": [
        "API",
        "library",
        "Pexels",
        "photo",
        "video",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/pexels-library",
    "license": "MIT",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0",
        "psr/log": "^1.0",
        "webeweb/provider-library": "^1.3",
        "webeweb/traits-library": "^1.0",
        "webeweb/types-library": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WBW\\Library\\Pexels\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Library\\Pexels\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/webeweb/pexels-library/issues"
    }
}
